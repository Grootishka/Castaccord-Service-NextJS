{
	"name": "Castaccord Service",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"build:analize": "ANALYZE=true next build",
		"start": "next start",
		"start:prod": "pm2 list && pm2-runtime start ecosystem.config.js",
		"lint": "npm run lint:js",
		"lint:js": "eslint src --fix",
		"prettier": "prettier --config .prettierrc \"./**/*.+(js|json|jsx)\" --write"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.js": [
			"eslint --fix"
		],
		"./**/*.+(js|json|jsx)": [
			"prettier --write"
		]
	},
	"dependencies": {
		"@react-oauth/google": "^0.12.2",
		"@redux-devtools/extension": "3.3.0",
		"cookies-next": "2.1.2",
		"isomorphic-fetch": "^3.0.0",
		"moment": "^2.30.1",
		"next": "12.3.4",
		"next-i18next": "^15.4.2",
		"next-redux-wrapper": "^7.0.5",
		"next-seo": "^4.28.1",
		"next-useragent": "^2.8.0",
		"prop-types": "^15.7.2",
		"qs": "^6.10.2",
		"query-string": "^6.14.0",
		"react": "18.0.0",
		"react-copy-to-clipboard": "^5.1.0",
		"react-datepicker": "^8.3.0",
		"react-datetime": "^3.3.1",
		"react-dom": "18.0.0",
		"react-redux": "7.2.9",
		"react-router-dom": "^5.2.0",
		"react-select": "^5.10.1",
		"react-slick": "^0.30.3",
		"react-toastify": "^11.0.5",
		"reactstrap": "^8.9.0",
		"redux": "4.1.0",
		"redux-thunk": "2.4.2",
		"slick-carousel": "^1.8.1"
	},
	"devDependencies": {
		"@babel/core": "^7.17.2",
		"@babel/eslint-parser": "^7.17.0",
		"@babel/plugin-transform-runtime": "^7.26.10",
		"@babel/preset-env": "^7.16.11",
		"@babel/preset-react": "^7.16.7",
		"@babel/runtime": "^7.27.0",
		"@next/bundle-analyzer": "^12.3.7",
		"@next/eslint-plugin-next": "^12.0.10",
		"@r-next-b/cli": "^0.1.1",
		"eslint": "^8.12.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-import": "^2.25.4",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-react": "^7.28.0",
		"husky": "^7.0.4",
		"lint-staged": "^10.5.4",
		"prettier": "^2.5.1",
		"sass": "1.63.6"
	}
